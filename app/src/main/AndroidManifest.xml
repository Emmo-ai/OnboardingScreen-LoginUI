<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emmoai.onboardingScreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.emmoai.onboardingScreen.Register"
            android:exported="false" />
        <activity
            android:name="com.emmoai.onboardingScreen.Login"
            android:exported="false" />
        <activity android:name="com.emmoai.onboardingScreen.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.emmoai.onboardingScreen.MainActivity" />
    </application>

</manifest>